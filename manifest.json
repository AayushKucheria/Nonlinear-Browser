{
  "name": "Nonlinear Browser",
  "description": "Representing browser tabs in a tree for contextual thought",
  "version": "0.1",
  "permissions": ["tabs"],
  "background": {
    "persistent": false,
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_title": "show nonlinear browser"
  },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' 'unsafe-eval' https://d3js.org/d3.v6.min.js; object-src 'self' "
}
