{
  "name": "Nonlinear Browser",
  "description": "Representing browser tabs in a tree for contextual thought",
  "version": "0.3",
  "permissions": ["tabs"],
  "background": {
    "persistent": false,
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": "res/logo/favicon.png",
    "default_title": "Open Nonlinear Browser"
  },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' 'unsafe-eval' https://d3js.org/d3.v6.min.js https://w.appzi.io/w.js https://w.appzi.io/v1-v2-compat.js 'https://fonts.googleapis.com/css?family=Roboto'; object-src 'self' "
}
